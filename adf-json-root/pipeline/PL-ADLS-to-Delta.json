{
	"name": "PL-ADLS-to-Delta",
	"properties": {
		"activities": [
			{
				"name": "Copy to Destination",
				"description": "copies data to delta tables",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Users/snoor@relisource.com/Delta Load",
					"baseParameters": {
						"source_path": {
							"value": "@pipeline().parameters.source_path",
							"type": "Expression"
						},
						"target_table": {
							"value": "@pipeline().parameters.target_table",
							"type": "Expression"
						},
						"load_type": {
							"value": "@pipeline().parameters.load_type",
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricks1",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Execute Pipeline2",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "Copy to Destination",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "PL-Delta-to-final",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"source": {
							"value": "@pipeline().parameters.target_table",
							"type": "Expression"
						},
						"target": {
							"value": "@pipeline().parameters.source_path",
							"type": "Expression"
						}
					}
				}
			}
		],
		"parameters": {
			"source_path": {
				"type": "string"
			},
			"target_table": {
				"type": "string"
			},
			"load_type": {
				"type": "string"
			},
			"watermarkstart": {
				"type": "string"
			},
			"watermarkmax": {
				"type": "string"
			}
		},
		"annotations": []
	}
}